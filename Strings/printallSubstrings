@Test
    //print all subsstring
    public void allSubs()
     {
         String s= "missisippie";
         for(int i=0;i<s.length();i++)
         {
             for(int j=i+1;j<=s.length();j++)
             {
                 System.out.println(s.substring(i,j));
             }
         }
     }

